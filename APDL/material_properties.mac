    MAT_G = 3770000
    !Temperature @68.0°F - 212°F
    MAT_CTE = 0.0000131                             
*ELSEIF,pipe_material_id,EQ,4,THEN
    MAT_d = 0.0979
    MAT_E = 10000000
    MAT_Nu = 0.33
    MAT_G = 3770000
    !Temperature @68.0°F - 212°F
